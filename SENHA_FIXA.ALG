Algoritmo "senha_fixa"

Var

   senha, senhadig : inteiro

Inicio

   senha <- 2002
   
   escreva("Digite a senha: ")
   leia(senhadig)

   enquanto senhadig <> senha faca
      escreva("Senha Invalida! Tente novamente: ")
      leia(senhadig)
   fimenquanto
   
   escreva("Acesso Permitido! ")

Fimalgoritmo